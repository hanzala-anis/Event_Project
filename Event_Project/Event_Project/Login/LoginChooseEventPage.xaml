             x:Class="Event_Project.Login.LoginChooseEventPage"
             NavigationPage.HasNavigationBar="False">
    <ContentPage.Content>
        <StackLayout>


            <StackLayout HorizontalOptions="Center" VerticalOptions="CenterAndExpand">
                <Image Source="@drawable/Icon"></Image>
                <Label Text="powered by GPS"></Label>
                <Picker x:Name="EventPicker_LoginChooseEventPage" SelectedIndexChanged="EventPicker_LoginChooseEventPage_OnSelectedIndexChanged"></Picker>
            </StackLayout>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>